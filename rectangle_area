#include <stdio.h>

int main()
{
  /*Variable*/
	double length, width;
  /*Length*/
	printf("Enter length: "); 
	scanf("%lf", &length);
  /*Width*/
	printf("Enter width: ");
	scanf("%lf", &width);
	
  /*Area*/
	double rectangle_area = length * width;
	
	printf("Rectangle area is %0.3lf.\n", rectangle_area);

	return 0;		
}
